{
  "name": "vue-jsx",
  "version": "0.0.1",
  "scripts": {
    "postinstall": "husky install && husky add .husky/pre-commit \"npm run lint-staged\"",
    "start": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit",
    "format": "prettier -w ./src",
    "eslint": "eslint --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore",
    "stylelint": "stylelint ./**/*.scss --ignore-path .gitignore",
    "lint-staged": "npm run format && lint-staged"
  },
  "dependencies": {
    "vue": "3.2.45"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "1.1.4",
    "@types/node": "18.11.9",
    "@vitejs/plugin-vue": "3.2.0",
    "@vitejs/plugin-vue-jsx": "2.1.1",
    "@vue/eslint-config-prettier": "7.0.0",
    "@vue/eslint-config-typescript": "11.0.0",
    "@vue/tsconfig": "0.1.3",
    "eslint": "8.22.0",
    "eslint-plugin-vue": "9.3.0",
    "husky": "8.0.3",
    "lint-staged": "13.1.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.7.1",
    "sass": "1.57.1",
    "stylelint": "14.16.1",
    "stylelint-config-standard": "29.0.0",
    "typescript": "4.7.4",
    "vite": "3.2.4",
    "vue-tsc": "1.0.9"
  }
}
